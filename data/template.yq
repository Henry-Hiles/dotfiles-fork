{
    "cache_s3_url": .b2_s3_api_url.value,
    "cache_bucket_name": .b2_cache_bucket_name.value,
    "hosts": .hosts.value | map_values(
        with_entries(
            select(
                .key == "wireguard_public_key" or
                .key == "syncthing_device_id"
            )
        )
    )
}
